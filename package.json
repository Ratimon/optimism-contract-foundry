  "devDependencies": {
    "@openzeppelin/contracts": "4.9.4",
    "@openzeppelin/contracts-upgradeable": "4.9.4",
    "as-soon": "^0.0.8",
    "clones-with-immutable-args": "1.0.0",
    "ldenv": "^0.3.9",
    "set-defaults": "^0.0.2",
    "solady": "0.0.174",
    "solmate": "v6"
  }
}